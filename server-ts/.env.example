# Logging
LOG_LEVEL=info

# Request Size Limits
REQUEST_SIZE_LIMIT_JSON=10mb
REQUEST_SIZE_LIMIT_URLENCODED=10mb
REQUEST_SIZE_LIMIT_RAW=100mb
REQUEST_SIZE_LIMIT_TEXT=1mb

WHITELIST=["127.0.0.1","::1"]